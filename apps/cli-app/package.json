{
  "name": "@one-logger/cli",
  "version": "0.1.0",
  "description": "CLI application for One Logger",
  "main": "dist/cli.js",
  "bin": {
    "one-logger": "dist/cli.js"
  },
  "scripts": {
    "build": "pnpm run build:web && tsc && pnpm run copy:web",
    "build:web": "cd ../vite-web-server && VITE_BUILD_SKIP_TS=true pnpm vite build --mode production || echo 'Web build failed, continuing without web assets'",
    "copy:web": "mkdir -p dist/web && cp -r ../vite-web-server/dist/* dist/web/ 2>/dev/null || echo 'Web build not found, skipping copy'",
    "dev": "tsx src/cli.ts start",
    "start": "node dist/cli.js",
    "type-check": "tsc --noEmit --skipLibCheck ",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.0",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^0.19.6",
    "@hono/zod-validator": "^0.2.0",
    "@libsql/client": "^0.15.7",
    "@modelcontextprotocol/sdk": "^1.11.4",
    "@one-logger/types": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.40.0",
    "express": "^5.1.0",
    "hono": "^3.12.0",
    "nanoid": "^5.0.4",
    "open": "^10.0.3",
    "ora": "^8.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/node": "^20.10.6",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.30.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "logging",
    "cli",
    "development"
  ],
  "author": "One Logger Team",
  "license": "MIT"
}